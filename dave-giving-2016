        event.preventDefault();
        popupwindow($(this).attr("href"), "sharePopup", 600, 300)
    }); 
            
    $('a.mailto-link-fix').click(function(){
        $(this).attr('href', $(this).attr('href').replace(/[^\?]+/i, "mailto:")); 
    }); 
});
    
