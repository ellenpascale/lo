    var emailShareHref = "mailto:?subject=Day of Giving&body=The Humane Society of the United States Day of Giving will benefit our Pets for Life Program. Every dollar raised provides pets in underserved communities the essential care they need such as vaccinations, spay and neuter, food, and more! http://bit.ly/1SwR7v5";
    
    var likeFrameSrc = "https://www.facebook.com/plugins/like.php?href=https%3A%2F%2Fsecure.humanesociety.org%2Fsite%2FSPageNavigator%2Fday_of_giving.html&send=false&layout=button_count&width&show_faces=false&action=like&height=35&appId=229366963819455";  
    
    
