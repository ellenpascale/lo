<script>
var terms = ["saves lives","helps pets in need","is a lifeline","strengthens communities","keeps families together"];
function rotateTerm() {
  var ct = $(".element").data("term") || 0;
  $(".element").data("term", ct == terms.length -1 ? 0 : ct + 1).text(terms[ct]).fadeIn()
              .delay(2000).fadeOut(200, rotateTerm);
}
$(rotateTerm);
</script>
