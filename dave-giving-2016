    var twitterShareHref = "https://twitter.com/intent/tweet?text=Today+is+%40HumaneSociety%27s+Day+of+Giving+benefitting+%23petsforlife%21+Join+us%3A+http%3A%2F%2Fbit.ly%2F1SwQZLZ+%23HSUSdayofgiving";
               
    var tumblrShareHref = "http://www.tumblr.com/share/link?url=https%3A%2F%2Fsecure.humanesociety.org%2Fsite%2FSPageNavigator%2Fday_of_giving.html%3Fs_src%3Dtm_share&name=Day+of+Giving&description=The+Humane+Society+of+the+United+States+Day+of+Giving+will+benefit+our+Pets+for+Life+Program.+Every+dollar+raised+provides+pets+in+underserved+communities+the+essential+care+they+need+such+as+vaccinations%2C+spay+and+neuter%2C+food%2C+and+more%21";
               
