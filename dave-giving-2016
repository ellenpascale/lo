    $('#dropdown-share-menu li.facebook a').attr('href', facebookShareHref);           
    
    $('.sharing-header li.twitter a').attr('href', twitterShareHref);
    $('#dropdown-share-menu li.twitter a').attr('href', twitterShareHref);           
    
    $('.sharing-header li.tumblr a').attr('href', tumblrShareHref);
    $('#dropdown-share-menu li.tumblr a').attr('href', tumblrShareHref);           
    
    $('.sharing-header li.pinterest a').attr('href', pinterestShareHref);
    $('#dropdown-share-menu li.pinterest a').attr('href', pinterestShareHref);           
    
    $('.sharing-header li.email a').attr('href', emailShareHref);
    $('#dropdown-share-menu li.email a').attr('href', emailShareHref);           
    
